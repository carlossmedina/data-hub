{
  "name": "pimcore/data-hub",
  "license": "GPL-3.0+",
  "type": "pimcore-bundle",
  "description": "Pimcore Datahub",
  "config": {
    "discard-changes": true
  },
  "autoload": {
    "psr-4": {
      "Pimcore\\Bundle\\DataHubBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "vendor/phpunit"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PimcoreDataHubBundle\\Tests\\": "tests"
    },
    "files": [
      "kernel/Kernel.php",
      "app/AppKernel.php"
    ]
  },
  "require": {
    "php": "^8.0",
    "pimcore/pimcore": "^10.2",
    "webonyx/graphql-php": "^14.6"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Pimcore\\Bundle\\DataHubBundle\\PimcoreDataHubBundle"
      ]
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4",
    "symfony/phpunit-bridge": "*",
    "codeception/codeception": "~2.4.5 || ^4.1.12"
  }
}
